{
  "name": "pickyourcourses-telegram-bot",
  "version": "1.0.0",
  "description": "Telegram bot for Ã‰cole Polytechnique students to share and read course reviews",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:prod": "serverless deploy --stage prod",
    "deploy:remove": "serverless remove",
    "logs": "serverless logs -f webhook",
    "invoke:local": "serverless invoke local -f webhook --path test-event.json",
    "package": "serverless package"
  },
  "keywords": [
    "telegram",
    "bot",
    "course",
    "reviews",
    "polytechnique",
    "aws",
    "lambda"
  ],
  "author": "PickYourCourses Team",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.1490.0",
    "node-telegram-bot-api": "^0.66.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.126",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/node-telegram-bot-api": "^0.64.7",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "serverless": "^3.38.0",
    "serverless-webpack": "^5.13.0",
